@model ICollection<Projekt.Models.Ban>

@using Projekt.Utils

@if (Model.Count > 0)
{

	<table class="table table-striped">
		<tr>
			<th class="col-md-2">Nazwa serwera</th>
			<th class="col-md-2">Powod</th>
			<th class="col-md-2">Dlugosc</th>
			<th class="col-md-2">Nick</th>
			<th class="col-md-2">Admin</th>
		</tr>
		@foreach (var item in Model)
		{
			@Html.Partial("~/Views/Manage/BanItem.cshtml", item)

		}

	</table>
}
else
{
	<h4>Nie masz żadnych banów! Cieszymy się, żę dogadujesz się z resztą naszej społeczności!</h4>
}